{
	"name": "phpgt/webengine",
	"description": "Rapid development engine.",

	"license": [
		"MIT"
	],

	"type": "library",

	"require": {
		"php": ">=7.2",
		"phpgt/build": "1.*",
		"phpgt/cli": "1.*",
		"phpgt/config": "1.*",
		"phpgt/cookie": "1.*",
		"phpgt/cron": "1.*",
		"phpgt/csrf": "1.*",
		"phpgt/database": "1.*",
		"phpgt/dom": "2.*",
		"phpgt/domtemplate": "1.*",
		"phpgt/fetch": "1.*",
		"phpgt/http": "1.*",
		"phpgt/input": "1.*",
		"phpgt/protectedglobal": "1.*",
		"phpgt/server": "1.*",
		"phpgt/session": "1.*",

		"psr/http-server-middleware": "^1.0",
		"willdurand/negotiation": "^2.3"
	},

	"require-dev": {
		"phpunit/phpunit": "8.*"
	},

	"autoload": {
		"psr-4": {
			"Gt\\WebEngine\\": "./src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Gt\\WebEngine\\Test\\": "./test/unit"
		}
	},

	"keywords": [
		"php",
		"phpgt",
		"php.gt",
		"gt",
		"framework",
		"webservice",
		"webengine",
		"router",
		"toolbox",
		"prototype"
	]
}
