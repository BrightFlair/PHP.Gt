{
	"name": "phpgt/webengine",
	"description": "Rapid development engine.",

	"license": [
		"MIT"
	],

	"type": "library",

	"require": {
		"ext-posix": "*",
		"ext-dom": "*",
		"ext-json": "*",
		"php": ">=8.0",
		"phpgt/build": "^1.0",
		"phpgt/cli": "^1.3",
		"phpgt/config": "^1.0",
		"phpgt/cookie": "^1.0",
		"phpgt/cron": "^1.0",
		"phpgt/csrf": "^1.6",
		"phpgt/database": "^1.1",
		"phpgt/dom": "^2.1",
		"phpgt/domtemplate": "^1.3",
		"phpgt/fetch": "^1.0",
		"phpgt/http": "^1.0",
		"phpgt/input": "^1.1",
		"phpgt/protectedglobal": "^1.0",
		"phpgt/server": "^1.1",
		"phpgt/session": "^1.1",
		"phpgt/sync": "^1.2",

		"psr/http-server-middleware": "^1.0",
		"willdurand/negotiation": "^3.0"
	},

	"require-dev": {
		"phpunit/phpunit": "8.* || 9.*",
		"behat/behat": "^3.0.5",
		"behat/mink-extension": "^2.3",
		"behat/mink-goutte-driver": "^1.2"
	},

	"autoload": {
		"psr-4": {
			"Gt\\WebEngine\\": "./src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Gt\\WebEngine\\Test\\": "./test/unit"
		}
	},

	"keywords": [
		"php",
		"phpgt",
		"php.gt",
		"gt",
		"framework",
		"webservice",
		"webengine",
		"router",
		"toolbox",
		"prototype"
	]
}
