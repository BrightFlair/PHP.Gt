{
	"name": "phpgt/webengine",
	"description": "Rapid development engine.",

	"license": [
		"MIT"
	],

	"type": "library",

	"require": {
		"ext-posix": "*",
		"ext-dom": "*",
		"php": ">=7.2",
		"phpgt/build": "*",
		"phpgt/cli": "*",
		"phpgt/config": "*",
		"phpgt/cookie": "*",
		"phpgt/cron": "*",
		"phpgt/csrf": "*",
		"phpgt/database": "*",
		"phpgt/dom": "*",
		"phpgt/domtemplate": "*",
		"phpgt/fetch": "*",
		"phpgt/http": "*",
		"phpgt/input": "*",
		"phpgt/protectedglobal": "*",
		"phpgt/server": "*",
		"phpgt/session": "*",

		"psr/http-server-middleware": "^1.0",
		"willdurand/negotiation": "^2.3"
	},

	"require-dev": {
		"phpunit/phpunit": "8.*",
		"behat/behat": "^3.0.5",
		"behat/mink-extension": "^2.3",
		"behat/mink-goutte-driver": "^1.2"
	},

	"autoload": {
		"psr-4": {
			"Gt\\WebEngine\\": "./src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Gt\\WebEngine\\Test\\": "./test/unit"
		}
	},

	"keywords": [
		"php",
		"phpgt",
		"php.gt",
		"gt",
		"framework",
		"webservice",
		"webengine",
		"router",
		"toolbox",
		"prototype"
	]
}
