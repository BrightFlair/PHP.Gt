{
	"name": "phpgt/webengine",

	"description": "Rapid development engine for PHP 7 applications.",

	"license": [
		"MIT"
	],

	"type": "library",

	"autoload": {
		"psr-4": {
			"Gt\\WebEngine\\": "./src/",
			"Gt\\Test\\WebEngine\\": "./test/unit/"
		}
	},

	"bin": [
		"bin/gt-create",
		"bin/gt-serve",
		"bin/gt-test",
		"bin/gt-deploy",
		"bin/gt-release"
	],

	"require": {
		"psr/http-server-middleware": "^1.0",
		"willdurand/negotiation": "^2.3",
		"symfony/css-selector": "3.2.*"
	},

	"require-dev": {
		"phpunit/phpunit": "6.4.*"
	},

	"keywords": [
		"php",
		"phpgt",
		"php.gt",
		"gt",
		"framework",
		"webservice",
		"webengine",
		"router",
		"toolbox",
		"prototype"
	]
}
