{
	"name": "phpgt/webengine",
	"description": "Rapid development engine.",

	"minimum-stability": "dev",
	"prefer-stable": true,

	"license": [
		"MIT"
	],

	"type": "library",

	"require": {
		"ext-posix": "*",
		"ext-dom": "*",
		"ext-json": "*",
		"php": ">=8.0",

		"phpgt/config": "^1.0",
		"phpgt/http": "^1.0",
		"phpgt/logger": "dev-master",
		"phpgt/routing": "dev-master",
		"phpgt/dom": "^3.0.0",
		"phpgt/domtemplate": "^2.0",

		"psr/http-server-middleware": "^1.0",
		"willdurand/negotiation": "^3.0"
	},

	"require-dev": {
		"phpstan/phpstan": "^0.12",
		"phpunit/phpunit": "^9.0",
		"behat/behat": "^3.0.5",
		"behat/mink-extension": "^2.3",
		"behat/mink-goutte-driver": "^1.2"
	},

	"autoload": {
		"psr-4": {
			"Gt\\WebEngine\\": "./src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Gt\\WebEngine\\Test\\": "./test/unit"
		}
	},

	"keywords": [
		"php",
		"phpgt",
		"php.gt",
		"gt",
		"framework",
		"webservice",
		"webengine",
		"router",
		"toolbox",
		"prototype"
	],

	"funding": [
		{
			"type": "github",
			"url": "https://github.com/sponsors/PhpGt"
		}
	]
}
