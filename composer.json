{
	"name": "phpgt/webengine",

	"description": "Rapid development engine for PHP 7 applications.",

	"license": [
		"MIT"
	],

	"type": "library",

	"autoload": {
		"psr-4": {
			"Gt\\WebEngine\\": "./src/",
			"Gt\\Test\\WebEngine\\": "./test/unit/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Gt\\WebEngine\\Test\\": "./test/unit"
		}
	},

	"bin": [
		"bin/gt-build",
		"bin/gt-serve",
		"bin/gt-run"
	],

	"require": {
		"phpgt/build": "dev-master",
		"phpgt/config": "dev-master",
		"phpgt/cookie": "dev-master",
		"phpgt/csrf": "dev-master",
		"phpgt/database": "dev-master",
		"phpgt/dom": "dev-master",
		"phpgt/domtemplate": "dev-master",
		"phpgt/http": "dev-master",
		"phpgt/input": "dev-master",
		"phpgt/protectedglobal": "dev-master",
		"phpgt/session": "dev-master",

		"psr/http-server-middleware": "^1.0",
		"willdurand/negotiation": "^2.3",
		"symfony/css-selector": "3.2.*"
	},

	"require-dev": {
		"phpunit/phpunit": "6.4.*"
	},

	"keywords": [
		"php",
		"phpgt",
		"php.gt",
		"gt",
		"framework",
		"webservice",
		"webengine",
		"router",
		"toolbox",
		"prototype"
	]
}
