{
	"name": "brightflair/php.gt",

	"description": "Web application development as it's meant to be.",

	"license": [
		"MIT"
	],

	"type": "library",

	"autoload": {
		"psr-4": {
			"Gt\\": "./src/"
		}
	},

	"bin": [
		"bin/serve",
		"bin/test"
	],
	"bin-dir": "./",

	"require": {
		"symfony/css-selector": "*",
		"symfony/console": "2.5.*",
		"leafo/scssphp": "0.0.*",
		"katzgrau/klogger": "1.0.*",
		"matthiasmullie/minify": "1.3.*",
		"michelf/php-markdown": "1.4.1",
		"g105b/phpcsv": "dev-master"
	},

	"require-dev": {
		"phpunit/phpunit": "4.1.*",
		"satooshi/php-coveralls": "dev-master",
		"behat/behat": "*",
		"behat/mink": "*",
		"behat/mink-extension": "*",
		"behat/mink-goutte-driver": "*",
		"sensiolabs/behat-page-object-extension": "*",
		"fabpot/goutte": "1.*"
	},

	"keywords": [
		"php",
		"phpgt",
		"gt",
		"web",
		"application",
		"framework",
		"dom",
