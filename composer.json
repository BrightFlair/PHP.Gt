{
	"name": "phpgt/webengine",

	"description": "Rapid development engine for PHP 7 applications.",

	"license": [
		"MIT"
	],

	"type": "library",

	"autoload": {
		"psr-4": {
			"Gt\\WebEngine\\": "./src/",
			"Gt\\Test\\WebEngine\\": "./test/unit/",

			"Gt\\Http\\": "./dev-dependencies/phpgt/http/src/",
			"Gt\\Cookie\\": "./dev-dependencies/phpgt/cookie/src/",
			"Gt\\Input\\": "./dev-dependencies/phpgt/input/src/",
			"Gt\\Session\\": "./dev-dependencies/phpgt/session/src/",
			"Gt\\Config\\": "./dev-dependencies/phpgt/config/src/",
			"Gt\\Build\\": "./dev-dependencies/phpgt/build/src/"
		}
	},

	"bin": [
		"bin/gt-create",
		"bin/gt-serve",
		"bin/gt-test",
		"bin/gt-deploy",
		"bin/gt-release"
	],

	"require": {
		"psr/http-message": "^1.0",
		"willdurand/negotiation": "^2.3"
	},

	"require-dev": {
		"phpunit/phpunit": "6.4.*"
	},

	"keywords": [
		"php",
		"phpgt",
		"php.gt",
		"gt",
		"framework",
		"webservice",
		"webengine",
		"router",
		"toolbox",
		"prototype"
	]
}
