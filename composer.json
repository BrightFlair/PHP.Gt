{
	"name": "brightflair/php.gt",

	"description": "Web application development as it's meant to be.",

	"license": [
		"Apache-2.0"
	],

	"type": "library",

	"autoload": {
		"psr-4": {
			"Gt\\": "./src/"
		}
	},

	"require": {
		"symfony/css-selector": "2.6.*",
		"symfony/console": "2.5.*",
		"leafo/scssphp": "0.0.*"
	},

	"require-dev": {
		"phpunit/phpunit": "4.1.*",
		"satooshi/php-coveralls": "dev-master",
		"behat/behat": "*",
		"behat/mink": "*",
		"behat/mink-extension": "*",
		"behat/mink-goutte-driver": "*",
		"sensiolabs/behat-page-object-extension": "*",
		"katzgrau/klogger": "1.0.*"
	},

	"prefer-stable": true,

	"keywords": [
		"php",
		"phpgt",
		"gt",
		"web",
		"application",
		"framework",
		"dom",
		"webservice",
		"service",
		"sass",
